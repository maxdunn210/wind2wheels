.nav.navbar.navbar-default.navbar-fixed-top{role: "navigation"}
  .container-fluid
    .navbar-header
      %button.navbar-toggle{"data-target" => "#navbar-ex1-collapse", "data-toggle" => "collapse", type: "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a.navbar-brand{href: "/"}
        = image_tag(image_path('w2w_logo_nav.png'), height: '50px')
      / Collect the nav links, forms, and other content for toggling.
      #navbar-ex1-collapse.collapse.navbar-collapse.navbar-right
        %ul.nav.navbar-nav
          %li
            = link_to 'My Cars', root_path
          %li
            = link_to 'Wind Simulator', utilities_path
          %li
            = link_to 'About Us', about_us_path
        %ul.nav.navbar-nav{style: 'margin-left:200px'}
          - if user_signed_in?
            %li
              = link_to "#{current_user.email}", user_path(current_user)
            %li
              = link_to 'Sign Out', destroy_user_session_path, :method => :delete
          -else
            %li
              = link_to 'Sign In', new_user_session_path
            %li
              = link_to 'Register', new_user_registration_path

      / /.navbar-collapse
    / /.container



